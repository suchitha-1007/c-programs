#include <stdio.h>

int main() {
   int i,n,r,sum,temp;
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        temp=i;
        sum=0;
        while(i!=0){
            r=i%10;
            sum=sum*10+r;
            i=i/10;
        }
        i=temp;
        if(sum==temp){
            printf("%d ",sum);
        }
    }
    return 0;
}
